{
  "name": "Blank new tab page",
  "description": "Override the new tab page with a blank one",
  "version": "0.2",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "blank.html"
  },
  "permissions": [
    "webRequest",
		"webRequestBlocking",
		"<all_urls>",
		"tabs",
		"topSites",
    "history"
  ],
  "content_scripts": [
    {
      "matches": ["http://www.qiushibaike.com/*"],
      "js": ["util.js", "inject.js"],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "browser_action": {},
  "content_security_policy": "script-src 'self' https://a.alipayobjects.com; object-src 'self'",
  "manifest_version": 2
}
